{
  "name": "frontend-ssr-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next build && npx serve@latest out",
    "git-hooks-install": "husky install",
    "storybook-start": "start-storybook -p 6006",
    "storybook-build": "build-storybook",
    "generate-ts-doc": "api-extractor run --local --verbose",
    "format": "prettier . --write",
    "lint:next": "next lint",
    "lint:tsc": "tsc",
    "lint:md": "markdownlint -i CHANGELOG.md **/*.md",
    "test:e2e": "cypress run --headless --browser chrome",
    "test:e2e-local": "cypress open",
    "test:unit": "jest --verbose",
    "test:memory-leak": "memlab run -v --scenario ./memlab/memoryLeakTest.js",
    "lighthouse:desktop": "next build && lhci autorun --config=lighthouserc.desktop.js --collect.startServerCommand=\"npx serve@latest out\"",
    "lighthouse:mobile": "next build && lhci autorun --config=lighthouserc.mobile.js --collect.startServerCommand=\"npx serve@latest out\"",
    "check-node-version": "node checkNodeVersion.js",
    "preinstall": "pnpm run check-node-version"
  },
  "dependencies": {
    "@apollo/client": "^3.12.9",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^6.4.2",
    "@mui/material": "^6.4.2",
    "@sentry/node": "^8.54.0",
    "@sentry/react": "^8.54.0",
    "cypress": "^14.0.1",
    "dotenv": "^16.4.7",
    "graphql": "^16.10.0",
    "i18next": "^24.2.2",
    "jest": "^29.7.0",
    "jsdoc": "^4.0.4",
    "next": "^15.1.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-i18next": "^15.4.0",
    "reflect-metadata": "^0.2.2",
    "require-from-string": "^2.0.2",
    "storybook": "^8.5.3",
    "terser-webpack-plugin": "^5.3.11",
    "tsyringe": "^4.8.0",
    "web-vitals": "^4.2.4",
    "webpack": "^5.97.1"
  },
  "devDependencies": {
    "@babel/core": "^7.26.7",
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
    "@lhci/cli": "^0.14.0",
    "@microsoft/api-extractor": "^7.49.1",
    "@next/eslint-plugin-next": "^15.1.6",
    "@storybook/addon-actions": "^8.5.3",
    "@storybook/addon-essentials": "^8.5.3",
    "@storybook/addon-interactions": "^8.5.3",
    "@storybook/addon-links": "^8.5.3",
    "@storybook/builder-webpack5": "^8.5.3",
    "@storybook/react": "^8.5.3",
    "@storybook/testing-library": "^0.2.2",
    "@testing-library/react": "^16.2.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.0",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@typescript-eslint/eslint-plugin": "^8.23.0",
    "@typescript-eslint/parser": "^8.23.0",
    "babel-loader": "^9.2.1",
    "eslint": "^9.19.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-next": "^15.1.6",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-cypress": "^4.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest-dom": "^5.5.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-storybook": "^0.11.2",
    "eslint-plugin-testing-library": "^7.1.1",
    "husky": "^9.1.7",
    "markdownlint-cli": "^0.44.0",
    "memlab": "^1.1.56",
    "prettier": "^3.4.2",
    "semver": "^7.7.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.7.3"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "engineStrict": true
}
